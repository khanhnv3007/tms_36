<table class="table">
  <thead>
    <tr>
      <th><%= t "subjects.name" %></th>
      <th><%= t "subjects.start_date" %></th>
      <th><%= t "subjects.end_date" %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @user_subjects %>
  </tbody>
</table>
<hr/>
<h2><%= t "instructions" %></h2>
<p class="container-fluid">
  <%= @course.description %>
</p>
<hr/>
<h2><%= t "courses.trainee" %>: </h2>
<% @course.users.each do |user| %>
  <%= render user %>
<% end %>
